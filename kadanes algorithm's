#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
     int arr[]={-2,1,-3,4,-1,2,1,-5,4};
     int n=9;
     int sum=0;
     for(int st=0;st<n;st++){
         sum=0;
         for(int end=st;end<n;end++){
             sum+=arr[end];
             cout << sum <<" ";
         }
         cout <<endl;
     }
    
}
// time O(n^2);
//spaceO(1)
